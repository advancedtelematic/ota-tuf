{
  "schedule": "$SCHEDULE",
  "name": "$JOB_NAME",
  "cpus": "0.5",
  "mem": "512",
  "fetch": [{
    "uri": "/root/docker.tar.gz"
  }],
  "container": {
    "type": "DOCKER",
    "image": "$DOCKER_IMAGE",
    "network": "BRIDGE",
    "forcePullImage": true
  },
  "environmentVariables": [
    {
      "name": "TUF_KEYSERVER_HOST",
      "value": "$TUF_KEYSERVER_HOST"
    },
    {
      "name": "TUF_KEYSERVER_PORT",
      "value": "$TUF_KEYSERVER_PORT"
    },
    {
      "name": "DB_MIGRATE",
      "value": "$DB_MIGRATE"
    },
    {
      "name": "DB_PASSWORD",
      "value": "$DB_PASSWORD"
    },
    {
      "name": "DB_URL",
      "value": "$DB_URL"
    },
    {
      "name": "DB_USER",
      "value": "$DB_USER"
    }
  ],
  "command": "-main com.advancedtelematic.tuf.reposerver.batch.RefreshTimestamps"
}
